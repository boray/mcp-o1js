    {
      "id": "1186287219816804503",
      "type": "Default",
      "timestamp": "2023-12-18T16:41:53.355+04:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "I feel that even if the new curve signature verification algorithm is currently implemented, it is still likely to be unable to be effectively adopted due to the limitations of circuit size and potential memory constraints in wasm.",
      "author": {
        "id": "1009392624966316144",
        "name": "zkbeta",
        "discriminator": "0000",
        "nickname": "zk",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/1009392624966316144/c82b3fa769ed6e6ffdea579381ed5f5c.png?size=512"
