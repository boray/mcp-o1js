      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "if you turn it into a tree structure you can make use of parallel proving across many machines",
      "author": {
        "id": "140202196758167552",
        "name": "las0",
        "discriminator": "0000",
        "nickname": "Las",
        "color": "#E5DFBB",
        "isBot": false,
        "roles": [
          {
            "id": "1280898028529778789",
            "name": "Community Builder",
